#include <iostream>

using namespace std;
class Matrix
{
    static int arr[3][3];
public:
    void set_matrix(int a[3][3])
    {
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
                cin>>arr[i][j];
        }
    }
    int* get_matrix()
    {
        return (&arr[0][0]);
    }
};
class Display
{
    int a[3][3];
public:
    void display(int *b)
    {
        for(int i=0;i<3;i++)
        {
            for(int j=0;j<3;j++)
            {
                cout<<*(b[i][j])<<" ";
            } cout<<endl;
    }
}
};

int main()
{
    int *arr;
  Matrix obj;
  obj.set_matrix();
  arr=obj.get_matrix();
  Display obj1;
  obj1.display(&arr[0][0]);
  return 0;
}


#include<iostream>
using namespace std;
class matrix
{
	int a[3][3];
	public :
	void getmatrix()
	{
		
		for(int i=0;i<3;i++)
		{ for(int j=0;j<3;j++)
		{
			cin>>a[i][j];
		}
		}
	}
	void display()
	{
		for(int i=0;i<3;i++)
		{ for(int j=0;j<3;j++)
		{
			cout<<a[i][j]<<" ";
		} cout<<endl;
		}
		
	}
};
int main()
{
	matrix a;
  cout<<"enter matrix";
	a.getmatrix();
  cout<< matrix you  entered is:<<endl;
	a.display();
	return 0;
}
